<h1>Events</h1>
<%= link_to "New Event", "events/new" %><br><%= link_to "Back","/" %><br><br>
<table border="1">
 <tr>
  <th>ID</th>
  <th>Event</th>
  <th>Venue</th>
  <th>Start Time</th>
  <th>End Time</th>
  <th>Description</th> 
</tr> 

 <% @events.each do |t| %>
  <tr>
   <td><%= t.id %></td>
   <td><%= t.event %></td>
   <td><%= t.venue %></td>
   <td><%= t.start_date %></td>
   <td><%= t.end_date %></td>
   <td><%= t.description %></td>
   <td><%= link_to 'Show', event_path(t) %></td>
   <td><%= link_to 'Edit', edit_event_path(t) %></td>
  </tr> 
 <% end %>
</table><br>



