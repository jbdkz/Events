<h1>Events</h1>

<table border="1">
 <tr>
  <th>ID</th>
  <th>Event</th>
  <th>Venue</th>
  <th>Start Time</th>
  <th>End Time</th>
  <th>Description</th> 
</tr> 

 <% @events.each do |t| %>
  <tr>
   <td><%= t.id %></td>
   <td><%= t.event %></td>
   <td><%= t.venue %></td>
   <td><%= t.start_date %></td>
   <td><%= t.end_date %></td>
   <td><%= t.description %></td>
  </tr> 
 <% end %>
</table><br>
<%= link_to "New Event", "index/new" %><br><br>

<%= link_to "Back", "/" %>
